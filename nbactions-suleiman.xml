<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:3.0.0:exec</goal>
            </goals>
            <properties>
                <exec.args>-classpath %classpath jade.MicroBoot -host suleiman.ujaen.es -port 8026 -agents AgenteOrganizadorNono:es.uja.ssmmaa.dots_and_boxes.agentes.AgenteOrganizador</exec.args>
                <exec.executable>java</exec.executable>
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:3.0.0:exec</goal>
            </goals>
            <properties>
                <exec.args>-agentlib:jdwp=transport=dt_socket,server=n,address=${jpda.address} -classpath %classpath jade.MicroBoot -host suleiman.ujaen.es -port 8026 -agents AgenteOrganizadorNono:es.uja.ssmmaa.dots_and_boxes.agentes.AgenteOrganizador</exec.args>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
            </properties>
        </action>
        <action>
            <actionName>profile</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:3.0.0:exec</goal>
            </goals>
            <properties>
                <exec.args>-classpath %classpath jade.MicroBoot -host suleiman.ujaen.es -port 8026 -agents AgenteOrganizadorNono:es.uja.ssmmaa.dots_and_boxes.agentes.AgenteOrganizador</exec.args>
                <exec.executable>java</exec.executable>
            </properties>
        </action>
    </actions>
